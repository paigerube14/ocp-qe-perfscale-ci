{
  "elasticsearch": {
    "ripsaw-kube-burner": [
      {
        "filter": {
          "metricName.keyword": "cpu-kubelet"
        },
        "buckets": [
          "jobName.keyword"
        ],
        "aggregations": {
          "value": [
            "avg"
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "memory-kubelet"
        },
        "buckets": [
          "jobName.keyword"
        ],
        "aggregations": {
          "value": [
            "avg"
          ]
        }
      }
    ]
  }
}